    "prettier": "npx prettier -w ./src",
    "build:lib": "cross-env NODE_ENV=production npx gulp",
    "preview": "node ./server/app.cjs",
    "test:browser": "cross-env TEST_ENV=browser vitest run",
    "test:v8": "cross-env TEST_ENV=nodejs vitest run",
    "test": "npm run test:browser && npm run test:v8"
  },
  "lint-staged": {
    "src/**/*{.tsx, .ts, .jsx, .js}": [
      "npm run lint",
      "npm run prettier",
      "git add ."
    ]
